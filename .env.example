# AI Provider Selection (gemini, together, fal, replicate, openai)
AI_PROVIDER=gemini

# Google Gemini API Key (Recommended - Great balance ~$0.010/image)
GEMINI_API_KEY=your_gemini_api_key_here

# Together.ai API Key (~$0.008/image)
TOGETHER_API_KEY=your_together_api_key_here

# Fal.ai API Key (~$0.055/image)
FAL_API_KEY=your_fal_api_key_here

# Replicate API Token (~$0.0023/image - Cheapest but slower)
REPLICATE_API_TOKEN=your_replicate_token_here

# OpenAI API Key (~$0.040/image)
OPENAI_API_KEY=your_openai_api_key_here

# Server Port (optional, defaults to 5000)
PORT=5000